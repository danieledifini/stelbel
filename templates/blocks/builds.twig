<div class="block-header dis-flex jus-space-between align-center">
    <h2 class="fdc fs-32 fs-42-desktop">
        {% if is_single_model %}
            {{options.related_builds}}
        {% else %}
            {{section.title}}
        {% endif %}
        
    </h2>
    {% if options.builds_cta %}
        <div class="ctas-container  ">
            <a  class="standard-btn foc fs-18 brdr-rds-20 "  href="{{options.builds_cta['url']}}" target="{{options.builds_cta['target']}}" >
                {{options.builds_cta['title']}}
            </a>
        </div>
    {% endif %}
</div>

{% set model_id = 0 %}
{% if is_single_model %}
    {% set model_id = post.id %}
{% endif %}

<div class="outer-list m-t-standard">
    <div class="builds-list  with-loading loading dis-flex "  id="builds-wrapper" hx-get="/wp-json/api/v1/latest_builds/build/{{ model_id }}"  hx-trigger="load">
    </div>
</div>